@model IEnumerable<TaskProgram.Models.Data>

@{
    ViewData["Title"] = "Data List";
}

<h2>Data List</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Add New Data</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Content</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Content</td>
            </tr>
        }
    </tbody>
</table>
